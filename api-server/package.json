{
  "name": "api-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "SET NODE_ENV=start && pm2 start bin/www",
    "restart": "pm2 restart bin/www",
    "stop": "pm2 delete bin/www",
    "dev-server": "SET NODE_ENV=dev && nodemon --inspect bin/www -V -w './'",
    "test": "SET NODE_ENV=test && mocha --timeout 10000"
  },
  "dependencies": {
    "@angular/animations": "^5.0.3",
    "@angular/cdk": "github:angular/cdk-builds",
    "@angular/material": "github:angular/material2-builds",
    "body-parser": "~1.18.2",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "config": "^1.28.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.15.5",
    "hammerjs": "^2.0.8",
    "mocha": "^4.0.1",
    "morgan": "^1.9.0",
    "serve-favicon": "~2.4.5",
    "sinon": "^4.1.2",
    "sinon-mongoose": "^2.0.2"
  },
  "devDependencies": {
    "@angular/cli": "^1.5.5",
    "axios": "^0.17.1",
    "jsonwebtoken": "^8.1.0",
    "mongoose": "^4.12.4",
    "node-fetch": "^1.7.3",
    "pm2": "^2.7.2",
    "r2": "^2.0.0"
  }
